<Window x:Class="OhjelmistokehitysProjekti.TestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OhjelmistokehitysProjekti"
        mc:Ignorable="d"
        Title="TestWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition MinWidth="200" MaxWidth="450"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <ListBox Grid.Column="0">
            <ListBoxItem>
                <Button Content="Command1" Command="{Binding myCommand}"></Button>

            </ListBoxItem>
            <ListBoxItem>
                <Button Content="Command2" Command="{Binding myCommand2}"></Button>

            </ListBoxItem>
        </ListBox>
    </Grid>
</Window>
